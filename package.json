{
  "name": "revite",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "demo": "vite --config demo/vite.config.ts",
    "demo:vue": "cross-env APP_CONFIG=vue yarn demo",
    "demo:next": "cross-env APP_CONFIG=next yarn demo",
    "demo:next:build": "cross-env APP_CONFIG=next vite build --config demo/vite.config.ts",
    "build": "tsc --project tsconfig.release.json",
    "test:unit": "vite-jest --no-cache",
    "release:prepare": "node --experimental-json-modules scripts/pre-release.mjs",
    "release": "semantic-release",
    "postinstall": "husky install",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "build:cli:revite": "tsc cli/revite.ts --outDir cli/dist --moduleResolution node",
    "revite": "revite"
  },
  "contributors": [
    "Roman Tebenkov <iteam4u@ya.ru>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/midrock/revite.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "autoprefixer": "^10.3.6",
    "babel-core": "^6.26.3",
    "commander": "^10.0.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.18.0",
    "generate-source-map": "^0.0.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "semantic-release": "^21.0.2",
    "tailwindcss": "^3.3.2",
    "terser": "^5.17.6",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-jest": "^0.1.4",
    "vite-plugin-markdown": "^2.1.0",
    "vue": "^3.3.4",
    "vue-jest": "^3.0.6",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.6.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "service",
    "provider",
    "ioc",
    "container",
    "bootstrap"
  ],
  "engines": {
    "node": ">=16.14"
  },
  "bin": {
    "revite": "cli/dist/revite.js"
  }
}
