{
  "name": "revite",
  "version": "1.15.0",
  "license": "MIT",
  "author": "Roman Tebenkov <iteam4u@ya.ru>",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && node --experimental-json-modules scripts/pre-release.mjs",
    "lint": "vue-tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "release": "semantic-release"
  },
  "main": "./dist/revite.es.js",
  "types": "./dist/revite.es.d.ts",
  "exports": {
    ".": {
      "types": "./dist/revite.es.d.ts",
      "import": "./dist/revite.es.js",
      "require": "./dist/revite.cjs.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": "./cli/bin/revite",
  "repository": {
    "type": "git",
    "url": "https://github.com/midrock/revite.git"
  },
  "bugs": {
    "url": "https://github.com/midrock/revite/issues"
  },
  "devDependencies": {
    "@revite/cli": "*",
    "@revite/vite-plugin": "*",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "semantic-release": "^21.0.2"
  },
  "keywords": [
    "service",
    "provider",
    "ioc",
    "container",
    "bootstrap"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
