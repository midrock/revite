{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["revite#build"],
      "cache": false
    },
    "build": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "release": {
      "cache": false,
      "dependsOn": ["^build", "build"]
    },
    "revite#build": {
      "dependsOn": ["^build"]
    }
  }
}
