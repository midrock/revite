{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["revite#build"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "release": {
      "cache": false,
      "dependsOn": ["^build", "build"]
    },
    "revite#build": {
      "dependsOn": ["^build"]
    },
    "test:unit": {
      "cache": false
    }
  }
}
